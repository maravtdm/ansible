hostname="bsd-one"
keymap="fr.kbd"
ifconfig_re0="inet 192.168.111.22 netmask 255.255.255.0"
defaultrouter="192.168.111.254"
sshd_enable="YES"
bsdstats_enable="YES"

# Set dumpdev to "AUTO" to enable crash dumps, "NO" to disable
dumpdev="AUTO"
zfs_enable="YES"
linux_enable="YES"

kld_list="radeonkms"
# /boot/modules/vboxnetflt.ko /boot/modules/vboxnetadp.ko"

hald_enable="YES"
microcode_update_enable="YES"

cnid_metad_enable="YES"
samba_server_enable="YES"
rpcbind_enable="YES"

chronyd_enable="YES"

###
dbus_enable="YES"
avahi_daemon_enable="YES"

# Firewall
pf_enable="YES"
pf_rules="/etc/pf.conf"
pflog_enable="NO"

#devfs_system_ruleset="localrules"
devfs_system_ruleset="system"

nginx_enable="YES"

jdownloader_enable="YES"
jdownloader_user="david"
jdownloader_group="david"

plexmediaserver_enable="YES"

ntpd_enable="NO"
slim_enable="NO"
powerdxx_enable="NO"
local_unbound_enable="NO"
#netatalk_enable="NO"
#nfs_server_enable="NO"
#mountd_enable="NO"
#nfs_server_flags="-u -t -n 4"
#mountd_flags="-r"

sendmail_enable="YES"
sendmail_submit_enable="NO"
sendmail_outbound_enable="NO"
sendmail_msp_queue_enable="YES"

#mysql_enable="NO"
#rundeckd_enable="NO"

#minidlna_enable="NO"
#clamav_clamd_enable="NO"
#clamav_freshclam_enable="NO"

#vboxservice_enable="NO"
#vboxnet_enable="NO"
#vboxguest_enable="NO"
#vboxheadless_enable="NO"
#vboxwebsrv_enable="NO"
#vboxwebsrv_flags="-F /tmp/vboxweb"

#wlans_ath0="wlan0"
#ifconfig_wlan0="WPA DHCP"
#smartd_enable="NO"

#vm_enable="YES"
#vm_dir="/mnt/zdoc/50_VM_Disk"
